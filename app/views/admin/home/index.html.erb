<%= content_for :header do %>
用户列表
<% end %>

<table class="table table-striped table-condensed table-bordered">
	<thead>
		<tr>
			<th>姓名</th>
			<th>Email</th>
			<th>登录方式</th>
			<th>最后登录时间</th>
			<th>当前登录时间</th>
			<th>最后登录IP</th>
			<th>登录次数</th>
		</tr>
	</thead>
	<% @users.each do |u| %>
		<tr>
			<td><%= u.name %></td>
			<td><%= u.email %></td>
			<td><%= u.provider %></td>
			<td><%= l u.last_sign_in_at %></td>
			<td><%= l u.current_sign_in_at %></td>
			<td><%= u.last_sign_in_ip %></td>
			<td><%= u.sign_in_count %></td>
		</tr>
	<% end %>
</table>

